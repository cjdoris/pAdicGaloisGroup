        func<args | PGGAlg_Conjugacy_Symmetric_Make(:GaloisGroupAlg:=args[1])>
      ),
      PGG_ArgSpec_Attr(
        "Factors",
        [<get("CONJUGACY"),"Next",false>],
        func<args | PGGAlg_Conjugacy_Factors_Make(:Next:=args[1])>
