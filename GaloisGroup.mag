end intrinsic;

intrinsic Hash(S :: PGGStat_AutGroup, v) -> .
  {"}
  return Hash(<Degree(Rep(v`value)), v`value>);
end intrinsic;

intrinsic Print(S :: PGGStat, v)
  {"}
  printf "%o", v`value;
end intrinsic;

intrinsic Print(S :: PGGStat_AutGroup, v)
  {"}
